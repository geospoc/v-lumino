{
  "name": "@geospoc/v-lumino",
  "version": "0.0.0",
  "description": "Vue Wrapper for awesome Lumino library",
  "types": "types/v-lumino.d.ts",
  "main": "dist/v-lumino.cjs.js",
  "module": "dist/v-lumino.esm.js",
  "unpkg": "dist/v-lumino.js",
  "jsdelivr": "dist/v-lumino.js",
  "cdn": "dist/v-lumino.min.js",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "test": "echo \"test\"",
    "build": "rimraf dist && npm run bundle && npm run bundle:min",
    "build:tailwind": "postcss styles/tailwind.css -o styles/bundle.css",
    "bundle": "rollup -c build/rollup.config.ts",
    "bundle:min": "rollup -c build/rollup.min.config.ts",
    "lint": "npm run lint:prettier && npm run lint:eslint && npm run lint:css",
    "lintfix": "npm run lint:prettier:fix && npm run lint:eslint:fix && npm run lint:css:fix",
    "lint:js": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{,!(node_modules|dist)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --fix \"{,!(node_modules|dist)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check \"{,!(node_modules|dist)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|dist)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:css": "stylelint \"{,!(node_modules|dist)/**/}*.{css,scss,vue}\" --ignore-path .gitignore",
    "lint:css:fix": "stylelint --fix \"{,!(node_modules|dist)/**/}*.{css,scss,vue}\" --ignore-path .gitignore",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "release": "shipjs prepare",
    "release:auto": "shipjs prepare --yes",
    "release:dry": "shipjs prepare --dry-run",
    "storybook": "start-storybook -p 3000",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@lumino/datagrid": "^0.35.1",
    "@lumino/default-theme": "^0.21.1",
    "@lumino/widgets": "^1.30.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-sucrase": "^4.0.2",
    "@storybook/addon-a11y": "^6.4.13",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^6.4.13",
    "@storybook/vue": "^6.4.13",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "@vue/composition-api": "^1.4.4",
    "babel-loader": "^8.2.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^37.6.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "rollup": "^2.64.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "5.1.9",
    "sass": "^1.49.0",
    "shipjs": "^0.24.1",
    "storybook-dark-mode": "^1.0.8",
    "storybook-tailwind-dark-mode": "^1.0.11",
    "stylelint": "^14.2.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.5.4",
    "vue": "^2.6.14",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14"
  },
